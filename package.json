{
  "name": "n8n-nodes-httpsoverproxy",
  "version": "0.2.6",
  "description": "n8n node to make HTTPS requests through HTTP proxy, solving the common 400 error",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "proxy",
    "https",
    "http",
    "axios"
  ],
  "license": "MIT",
  "homepage": "https://github.com/FunTW/n8n-nodes-httpsoverproxy",
  "author": {
    "name": "FunTW",
    "email": "26371057+FunTW@users.noreply.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FunTW/n8n-nodes-httpsoverproxy.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:all",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "files": [
    "dist",
    "nodes",
    "index.js"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/HttpsOverProxy/HttpsOverProxy.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.27.0",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@types/lodash": "^4.17.17",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
